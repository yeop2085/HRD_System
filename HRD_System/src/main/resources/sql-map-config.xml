<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<typeAlias type="com.green.biz.employee.EmployeeVO" alias="employee"/>
		<typeAlias type="com.green.biz.hrdAdmin.HrdAdminVO" alias="hrdAdmin"/>
		<typeAlias type="com.green.biz.information.InformationVO" alias="information"/>
		<typeAlias type="com.green.biz.disciplinary.DisciplinaryVO" alias="disciplinary"/>
		<typeAlias type="com.green.biz.vacation.VacationVO" alias="vacation"/>
